{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/auth/.*",
      "dest": "server.js"
    },
    {
      "src": "/api/.*",
      "dest": "server.js"
    },
    {
      "src": "/sw.js",
      "dest": "server.js"
    },
    {
      "src": "/locales/.*",
      "dest": "server.js"
    },
    {
      "src": "/icons/.*",
      "dest": "server.js"
    },
    {
      "src": "/.*",
      "dest": "server.js"
    }
  ]
}
