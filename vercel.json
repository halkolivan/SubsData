{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    
    {
      "src": "/auth/github",
      "dest": "server.js"
    },
    
    {
      "src": "/(sw.js|favicon.ico)",
      "dest": "dist/$1"
    },
    {
      "src": "/(locales|icons|assets)/(.*)",
      "dest": "dist/$1/$2"
    },
    
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}
